    "jspdf-autotable": "^3.8.4",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.453.0",
    "next": "^15.0.3",
    "next-auth": "^4.24.8",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.15",
    "pdfkit": "^0.15.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.3.0",
