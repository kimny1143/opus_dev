{
  "name": "opus_dev",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "clear-cache": "rm -rf .next/cache",
    "dev:clear": "npm run clear-cache && npm run dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "seed": "node --loader ts-node/esm prisma/seed.mts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name dev",
    "prisma:migrate:prod": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma": "prisma",
    "postinstall": "prisma generate",
    "test": "dotenv -e .env.test -- npx jest",
    "test:watch": "dotenv -e .env.test -- npx jest --watch",
    "test:coverage": "dotenv -e .env.test -- npx jest --coverage",
    "test-log": "npm test > test-results.txt 2>&1",
    "test-tee": "npm test 2>&1 | tee test-results.txt"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
